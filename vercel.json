{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "crons": [
        {
          "path": "/evento/sendEventosSemanales",
          "schedule": "00 10 * * 1"
        },
        {
          "path": "/evento/remindEvento",
          "schedule": "0 10 * * *"
        },
        {
          "path": "/evento/sendEventosDiarios",
          "schedule": "00 14 * * 2-7"
        }
      ]
}
